# Pulumi Configuration
PULUMI_CONFIG_PASSPHRASE=your-secure-passphrase-here

# AWS Configuration for Pulumi Deployment
# Copy this file to .env and fill in your values

# AWS Profile to use (should have appropriate permissions)
AWS_PROFILE=your-aws-profile-name

# AWS Region for deployment
AWS_REGION=us-east-1

# Pulumi stack name
STACK_NAME=dev

# Project Configuration
PROJECT_NAME=azure-calculator
ENVIRONMENT=dev

# Lambda Configuration
LAMBDA_MEMORY_SIZE=1024
LAMBDA_TIMEOUT=30
LAMBDA_DESCRIPTION=Azure Calculator Lambda Function
